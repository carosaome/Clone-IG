<div class="body">
    <div class="container__body grid wide">
        <div class="left">
            <div class="body__story">
                <ul class="container__story">
                    <li class="story__item">
                        <div class="story__img"
                            style="background-image: url(data:image/png;base64,);">

                        </div>
                        <span class="story__user-name">
                            carosaome
                        </span>
                    </li>
                    <li class="story__item">
                        <div class="story__img"
                            style="background-image: url(data:image/png;base64,);">

                        </div>
                        <span class="story__user-name">
                            carosaome
                        </span>
                    </li>
                    <li class="story__item">
                        <div class="story__img"
                            style="background-image: url(data:image/png;base64,);">

                        </div>
                        <span class="story__user-name">
                            carosaome
                        </span>
                    </li>
                    <li class="story__item">
                        <div class="story__img"
                            style="background-image: url(data:image/png;base64,);">

                        </div>
                        <span class="story__user-name">
                            carosaome
                        </span>
                    </li>
                </ul>
            </div>
            <div class="body__newsfeed">
                {{#each posts}}
                <div class="container__news">
                    <div class="header__news">
                        <div class="header__infor">
                            <div class="header__infor-user-img">
                                <img src="{{this.createdBy.avatar}}" alt="">
                            </div>
                            <div class="header__infor-user-name">
                                {{this.createdBy.username}}
                            </div>
                        </div>
                        <div class="header__options">
                            <svg aria-label="More options" class="_8-yf5 " color="#262626" fill="#262626" height="24"
                                role="img" viewBox="0 0 24 24" width="24">
                                <circle cx="12" cy="12" r="1.5"></circle>
                                <circle cx="6" cy="12" r="1.5"></circle>
                                <circle cx="18" cy="12" r="1.5"></circle>
                            </svg>
                        </div>

                    </div>
                    <div class="news__main-img">
                        <a href="/post/{{this._id}}">
                            <img src="{{this.imgUrl}}" alt="">
                        </a>
                    </div>
                    <div class="footer__newsfeed">
                        <div class="news__actions">
                            <div class="news__actions-left">
                                <span class="news__actions-heart">
                                    <svg aria-label="Like" class="_8-yf5 " color="#262626" fill="#262626" height="24"
                                        role="img" viewBox="0 0 24 24" width="24">
                                        <path
                                            d="M16.792 3.904A4.989 4.989 0 0121.5 9.122c0 3.072-2.652 4.959-5.197 7.222-2.512 2.243-3.865 3.469-4.303 3.752-.477-.309-2.143-1.823-4.303-3.752C5.141 14.072 2.5 12.167 2.5 9.122a4.989 4.989 0 014.708-5.218 4.21 4.21 0 013.675 1.941c.84 1.175.98 1.763 1.12 1.763s.278-.588 1.11-1.766a4.17 4.17 0 013.679-1.938m0-2a6.04 6.04 0 00-4.797 2.127 6.052 6.052 0 00-4.787-2.127A6.985 6.985 0 00.5 9.122c0 3.61 2.55 5.827 5.015 7.97.283.246.569.494.853.747l1.027.918a44.998 44.998 0 003.518 3.018 2 2 0 002.174 0 45.263 45.263 0 003.626-3.115l.922-.824c.293-.26.59-.519.885-.774 2.334-2.025 4.98-4.32 4.98-7.94a6.985 6.985 0 00-6.708-7.218z">
                                        </path>
                                    </svg>

                                </span>
                                <span class="news__actions-comment">
                                    <svg aria-label="Comment" class="_8-yf5 " color="#262626" fill="#262626" height="24"
                                        role="img" viewBox="0 0 24 24" width="24">
                                        <path d="M20.656 17.008a9.993 9.993 0 10-3.59 3.615L22 22z" fill="none"
                                            stroke="currentColor" stroke-linejoin="round" stroke-width="2"></path>
                                    </svg>
                                </span>
                                <span class="news__actions-share">
                                    <svg aria-label="Share Post" class="_8-yf5 " color="#262626" fill="#262626"
                                        height="24" role="img" viewBox="0 0 24 24" width="24">
                                        <line fill="none" stroke="currentColor" stroke-linejoin="round" stroke-width="2"
                                            x1="22" x2="9.218" y1="3" y2="10.083"></line>
                                        <polygon fill="none"
                                            points="11.698 20.334 22 3.001 2 3.001 9.218 10.084 11.698 20.334"
                                            stroke="currentColor" stroke-linejoin="round" stroke-width="2">
                                        </polygon>
                                    </svg>
                                </span>
                            </div>
                            <div class="news__actions-right">
                                <span class="news__actions-save">
                                    <svg aria-label="Save" class="_8-yf5 " color="#262626" fill="#262626" height="24"
                                        role="img" viewBox="0 0 24 24" width="24">
                                        <polygon fill="none" points="20 21 12 13.44 4 21 4 3 20 3 20 21"
                                            stroke="currentColor" stroke-linecap="round" stroke-linejoin="round"
                                            stroke-width="2"></polygon>
                                    </svg>
                                </span>
                            </div>
                        </div>

                        <div class="news__likes">
                            234 likes
                        </div>

                        <div class="news__content">
                            <span class="news__content-user-name">{{this.createdBy.username}}</span>
                            <span class="content">{{this.title}}</span>
                        </div>
                        <div class="news__views__comment">
                            Views all <span class="news__views__num-comment"> {{this.totalComments}} </span> comments
                        </div>
                        <div class="news__post-time">
                            {{this.createdAt}}
                        </div>
                    </div>
                    <div class="container__comment">
                        <form id="{{this._id}}" class="form__comment">

                            <div class="form__comment-left">
                                <svg aria-label="Emoji" class="_8-yf5 " color="#262626" fill="#262626" height="24"
                                    role="img" viewBox="0 0 24 24" width="24">
                                    <path
                                        d="M15.83 10.997a1.167 1.167 0 101.167 1.167 1.167 1.167 0 00-1.167-1.167zm-6.5 1.167a1.167 1.167 0 10-1.166 1.167 1.167 1.167 0 001.166-1.167zm5.163 3.24a3.406 3.406 0 01-4.982.007 1 1 0 10-1.557 1.256 5.397 5.397 0 008.09 0 1 1 0 00-1.55-1.263zM12 .503a11.5 11.5 0 1011.5 11.5A11.513 11.513 0 0012 .503zm0 21a9.5 9.5 0 119.5-9.5 9.51 9.51 0 01-9.5 9.5z">
                                    </path>
                                </svg>

                                <input name="content" type="text" placeholder="Add a comment...">
                            </div>
                            <button class="btn btn__post-comment">Post</button>
                        </form>
                    </div>
                </div>

                {{/each}}
            </div>
        </div>
        <div class="right">
            <div class="container__right">
                <div class="container__user">
                    <div class="container__img">
                        <img src="data:image/png;base64,"
                            alt="">

                    </div>
                    <div class="container__name">
                        <span class="name">carosaome</span>
                        <span class="desc-name">call me caros</span>
                    </div>
                    <button class=" btn-switch">Switch</button>
                </div>
                <div class="user-suggestion">
                    <span class="content__suggestion">Suggestions For You</span>
                    <button class="btn-suggestion">See All</button>
                </div>
                <div class="container__user-suggestion">
                    <div class="container__user-suggestion_item">
                        <div class="container__img">
                            <img src="data:image/png;base64,"
                                alt="">

                        </div>
                        <div class="container__name">
                            <div class="name">carosaome</div>
                            <div class="user-follow">Follows you</div>
                        </div>
                        <button class="btn-switch">Follow</button>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>

<script>


    function handleScroll() {
        let offset = 1
        const bodyContent = document.querySelector('.body__newsfeed')
        window.onscroll = async function () {
            let check = document.body.scrollHeight - window.innerHeight
            if (window.pageYOffset == check) {
                offset = offset + 1
                console.log(offset)
                let params = {
                    "offset": offset,
                };

                let query = Object.keys(params)
                    .map(k => encodeURIComponent(k) + '=' + encodeURIComponent(params[k]))
                    .join('&');
                await fetch('/posts' + '?' + query)
                    .then((res) => res.json())
                    .then((data) => {
                        data.map((post) => {
                            bodyContent.innerHTML += `
                             <div class="container__news">
                    <div class="header__news">
                        <div class="header__infor">
                            <div class="header__infor-user-img">
                                <img src="${post.createdBy.avatar}"
                                    alt="">
                            </div>
                            <div class="header__infor-user-name">
                                ${post.createdBy.username}
                            </div>
                        </div>
                        <div class="header__options">
                            <svg aria-label="More options" class="_8-yf5 " color="#262626" fill="#262626" height="24"
                                role="img" viewBox="0 0 24 24" width="24">
                                <circle cx="12" cy="12" r="1.5"></circle>
                                <circle cx="6" cy="12" r="1.5"></circle>
                                <circle cx="18" cy="12" r="1.5"></circle>
                            </svg>
                        </div>

                    </div>
                    <div class="news__main-img">
                        <a href="/post/${post._id}">
                            <img src="${post.imgUrl}" alt="">
                        </a>
                    </div>
                    <div class="footer__newsfeed">
                        <div class="news__actions">
                            <div class="news__actions-left">
                                <span class="news__actions-heart">
                                    <svg aria-label="Like" class="_8-yf5 " color="#262626" fill="#262626" height="24"
                                        role="img" viewBox="0 0 24 24" width="24">
                                        <path
                                            d="M16.792 3.904A4.989 4.989 0 0121.5 9.122c0 3.072-2.652 4.959-5.197 7.222-2.512 2.243-3.865 3.469-4.303 3.752-.477-.309-2.143-1.823-4.303-3.752C5.141 14.072 2.5 12.167 2.5 9.122a4.989 4.989 0 014.708-5.218 4.21 4.21 0 013.675 1.941c.84 1.175.98 1.763 1.12 1.763s.278-.588 1.11-1.766a4.17 4.17 0 013.679-1.938m0-2a6.04 6.04 0 00-4.797 2.127 6.052 6.052 0 00-4.787-2.127A6.985 6.985 0 00.5 9.122c0 3.61 2.55 5.827 5.015 7.97.283.246.569.494.853.747l1.027.918a44.998 44.998 0 003.518 3.018 2 2 0 002.174 0 45.263 45.263 0 003.626-3.115l.922-.824c.293-.26.59-.519.885-.774 2.334-2.025 4.98-4.32 4.98-7.94a6.985 6.985 0 00-6.708-7.218z">
                                        </path>
                                    </svg>

                                </span>
                                <span class="news__actions-comment">
                                    <svg aria-label="Comment" class="_8-yf5 " color="#262626" fill="#262626" height="24"
                                        role="img" viewBox="0 0 24 24" width="24">
                                        <path d="M20.656 17.008a9.993 9.993 0 10-3.59 3.615L22 22z" fill="none"
                                            stroke="currentColor" stroke-linejoin="round" stroke-width="2"></path>
                                    </svg>
                                </span>
                                <span class="news__actions-share">
                                    <svg aria-label="Share Post" class="_8-yf5 " color="#262626" fill="#262626"
                                        height="24" role="img" viewBox="0 0 24 24" width="24">
                                        <line fill="none" stroke="currentColor" stroke-linejoin="round" stroke-width="2"
                                            x1="22" x2="9.218" y1="3" y2="10.083"></line>
                                        <polygon fill="none"
                                            points="11.698 20.334 22 3.001 2 3.001 9.218 10.084 11.698 20.334"
                                            stroke="currentColor" stroke-linejoin="round" stroke-width="2">
                                        </polygon>
                                    </svg>
                                </span>
                            </div>
                            <div class="news__actions-right">
                                <span class="news__actions-save">
                                    <svg aria-label="Save" class="_8-yf5 " color="#262626" fill="#262626" height="24"
                                        role="img" viewBox="0 0 24 24" width="24">
                                        <polygon fill="none" points="20 21 12 13.44 4 21 4 3 20 3 20 21"
                                            stroke="currentColor" stroke-linecap="round" stroke-linejoin="round"
                                            stroke-width="2"></polygon>
                                    </svg>
                                </span>
                            </div>
                        </div>

                        <div class="news__likes">
                            234 likes
                        </div>

                        <div class="news__content">
                            <span class="news__content-user-name">${post.createdBy.username}</span>
                            <span class="content">${post.title}</span>
                        </div>
                        <div class="news__views__comment">
                            Views all <span class="news__views__num-comment"> ${post.totalComments} </span> comments
                        </div>
                        <div class="news__post-time">
                            ${post.createdAt}
                        </div>
                    </div>
                    <div class="container__comment">
                        <form id="${post._id}" class="form__comment">

                            <div class="form__comment-left">
                                <svg aria-label="Emoji" class="_8-yf5 " color="#262626" fill="#262626" height="24"
                                    role="img" viewBox="0 0 24 24" width="24">
                                    <path
                                        d="M15.83 10.997a1.167 1.167 0 101.167 1.167 1.167 1.167 0 00-1.167-1.167zm-6.5 1.167a1.167 1.167 0 10-1.166 1.167 1.167 1.167 0 001.166-1.167zm5.163 3.24a3.406 3.406 0 01-4.982.007 1 1 0 10-1.557 1.256 5.397 5.397 0 008.09 0 1 1 0 00-1.55-1.263zM12 .503a11.5 11.5 0 1011.5 11.5A11.513 11.513 0 0012 .503zm0 21a9.5 9.5 0 119.5-9.5 9.51 9.51 0 01-9.5 9.5z">
                                    </path>
                                </svg>

                                <input name="content" type="text" placeholder="Add a comment...">
                            </div>
                            <button class="btn btn__post-comment">Post</button>
                        </form>
                    </div>
                </div>
                        
                        `
                        })
                    })
                    .then(()=>{
                        handleSubmitForm()
                    })

            }

        }
    }
    handleScroll()

    function handleSubmitForm() {
        let formSubmit = document.querySelectorAll('.form__comment')
        formSubmit.forEach((item) => {

            item.addEventListener('submit', async (e) => {
                e.preventDefault()
                const postId = item.getAttribute('id')
                const content = item.elements['content'].value
                const data = { postId, content }
                const sendComment = await fetch('/api/comment', {
                    method: 'POST', // *GET, POST, PUT, DELETE, etc.
                    mode: 'cors', // no-cors, *cors, same-origin
                    cache: 'no-cache', // *default, no-cache, reload, force-cache, only-if-cached
                    credentials: 'same-origin', // include, *same-origin, omit
                    headers: {
                        'Content-Type': 'application/json'
                        // 'Content-Type': 'application/x-www-form-urlencoded',
                    },
                    redirect: 'follow', // manual, *follow, error
                    referrerPolicy: 'no-referrer', // no-referrer, *no-referrer-when-downgrade, origin, origin-when-cross-origin, same-origin, strict-origin, strict-origin-when-cross-origin, unsafe-url
                    body: JSON.stringify(data)
                })
                    .then((res) => {
                        res.json()
                    })
                    .then((data) => {
                        console.log(data)
                    })

            })
        })
    }
    handleSubmitForm()

</script>