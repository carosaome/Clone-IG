<div class="messager">
  <div class="grid wide box__messager">
    <div class="box__messager__chatMenu col l-4">
      {{#each userActive}}
      <div id="{{this._id}}" class="chatMenuWrapper">
        <div class="chatMenuWrapper__img">
          <img src="{{this.avatar}}" alt="This is avatar">
        </div>
        <div class="chatMenuWrapper__username">
          {{this.username}}
        </div>
        <div class="chatMenuWrapper__online-budge"></div>
      </div>
      {{/each}}
    </div>

    <div class="box__messager__chatBox l-8">
      <h1 style="text-align: center;">Click to start converstaion</h1>
    </div>
  </div>
</div>

<script src="/socket.io/socket.io.js"></script>
<script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>

<script>

  const socket = io()
  const listConverstations = document.querySelectorAll('.chatMenuWrapper')
  listConverstations.forEach((converstation) => {
    let guestId = converstation.getAttribute('id')
    converstation.addEventListener('click', () => {
      axios({
        method: 'post',
        url: '/message',
        data: {
          guestId
        }
      })
        .then((res) => {
          const converstaionId = res.data.foundCon._id
          const guestAvt = res.data.foundGuest.avatar
          const guestUsername = res.data.foundGuest.username
          const boxMessgae = document.querySelector('.box__messager__chatBox')
          boxMessgae.innerHTML = `
              <div class="chatBox__header">
                  <div class="chatBox__header-img">
                    <img
                      src="${guestAvt}"
                      alt="">
                  </div>
                  <span class="chatBox__header-username">
                    ${guestUsername}
                  </span> 
              </div>
              <div class="chatBox__body">
               

              </div>
              <div id="${converstaionId}" class="chatBox__submitForm">
                <form class="submitForm" action="">
                  <input id="input" placeholder="Message..." name="content" class="chatBox__submitForm__input" type="text">
                  <button class="chatBox__submitForm__button">
                    Send
                  </button>
                </form>
              </div>
          
          `
          return res
        })
        .then((res) => {
          const foundMessages = res.data.foundMessages
          const guestId = res.data.foundGuest._id
          const boxMessage = document.querySelector('.chatBox__body')
          const formSubmit = document.querySelector('.chatBox__submitForm')

          if (foundMessages.length > 0) {
            foundMessages.map((mess) => {
              boxMessage.innerHTML += `
                  <div class="messageWrapper ${mess.createdBy !== guestId ? 'own' : ''}">
                        <div class="message-img">
                          <img
                            src="data:image/png;base64,"
                            alt="">
                        </div>
                        <div class="messageWrapper-content">
                          <span class="messageWrapper-text">
                            ${mess.content}
                          </span>
                        </div>
                        <div class="messageWrapper-time">
                          <span class="timeAgo"> 10 days</span> ago
                  </div>
          `

            })
          }
          else {
            boxMessage.innerHTML = ``
          }

          formSubmit.addEventListener('submit', async (e) => {
            e.preventDefault()
            const converstationId = formSubmit.getAttribute('id')
            const content = document.getElementById("input").value;
            const data = { converstationId, content }
            if (content) {
              await axios({
                method: 'post',
                url: '/message/sendMessage',
                data: data
              })
                .then((res) => {
                  console.log(socket.id)
                  res.data.socketId = socket.id
                  socket.emit('send-message', res.data);

                })

              input.value = ''
            }

          });

          socket.on('getMessage', (data)=>{
            console.log(data, guestId)
            const boxMessage = document.querySelector('.chatBox__body')
            const item = document.createElement('div')
            item.classList.add('container__messages')
            item.innerHTML = `
                   <div class="messageWrapper ${guestId !== data.createdBy ? 'own': ''}">
                        <div class="message-img">
                          <img
                            src="data:image/png;base64,"
                            alt="">
                        </div>
                        <div class="messageWrapper-content">
                          <span class="messageWrapper-text">
                            ${data.content}
                          </span>
                        </div>
                        <div class="messageWrapper-time">
                          <span class="timeAgo"> 10 days</span> ago
                  </div>

            `
            boxMessage.appendChild(item)
            window.scrollTo(0, document.body.scrollHeight);
            console.log(data)
          })
        })
    })
  })
</script>


