<div class="messager">
  <div class="grid wide box__messager">
    <div class="box__messager__chatMenu col l-4 m-2">
      {{#each userActive}}
      <a id="chatmenuWrapper" href="/message/{{this._id}}">
        <div id="{{this._id}}" class="chatMenuWrapper">
          <div class="chatMenuWrapper__img">
            <img src="/{{this.avatar}}" alt="This is avatar">
          </div>
          <div class="chatMenuWrapper__username">
            {{this.username}}
          </div>
          <div class="chatMenuWrapper__online-budge"></div>
        </div>
      </a>
      {{/each}}
    </div>

    {{#if foundMessages}}
    <div class="box__messager__chatBox l-8 m-10">
      <div id="{{foundGuest._id}}" class="chatBox__header">
        <div class="chatBox__header-img">
          <img src="/{{foundGuest.avatar}}" alt="">
        </div>
        <span class="chatBox__header-username">
          {{foundGuest.username}}
        </span>
      </div>
      <div class="chatBox__body">
        {{#each foundMessages }}

        <div class="messageWrapper" id="{{this.createdBy}}">
          <div class="message-img">
            <img
              src="data:image/png;base64,"
              alt="">
          </div>
          <div class="messageWrapper-content">
            <span class="messageWrapper-text">
              {{this.content}}
            </span>
          </div>
          <div class="messageWrapper-time">
            <span class="timeAgo"> 10 days</span> ago
          </div>
        </div>
        {{/each}}
      </div>
      <div class="chatBox__submitForm">
        <form id="{{foundConId}}" class="submitForm" action="">
          <input id="input" placeholder="Message..." name="content" class="chatBox__submitForm__input" type="text">
          <button class="chatBox__submitForm__button">
            Send
          </button>
        </form>
      </div>
    </div>
    {{else}}
    <div class="box__messager__chatBox l-8 m-10">
      <div class="chatBox__header">
        <div class="chatBox__header-img">
          <img
            src="/{{foundGuest.avatar}}"
            alt="">
        </div>
        <span class="chatBox__header-username">
          {{foundGuest.username}}
        </span>
      </div>
      <div class="chatBox__body">

      </div>
      <div class="chatBox__submitForm">
        <form id="{{foundConId}}" class="submitForm" action="">
          <input id="input" placeholder="Message..." name="content" class="chatBox__submitForm__input" type="text">
          <button class="chatBox__submitForm__button">
            Send
          </button>
        </form>
      </div>
    </div>

    {{/if}}
  </div>
</div>

<script src="/socket.io/socket.io.js"></script>
<script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>

<script>

  const socket = io.connect('http://localhost:9999/')
  const formSubmit = document.querySelector('.submitForm')
  const roomId = formSubmit.getAttribute('id')
  const guestId = document.querySelector('.chatBox__header').getAttribute('id')
  const chatmenuWrapper = document.getElementById('chatmenuWrapper')
  const listMessages = document.querySelectorAll('.messageWrapper')

  // check message own or guest
  listMessages.forEach((message) => {
    const messageId = message.id
    if (messageId !== guestId) {
      message.classList.add('own')
    }
  })

  //join room 
  socket.emit('join-room', roomId)



// handle submit
  formSubmit.addEventListener('submit', async (e) => {
    e.preventDefault()
    const converstationId = formSubmit.getAttribute('id')
    const content = document.getElementById("input").value;
    const data = { converstationId, content }
    if (content) {
      await axios({
        method: 'post',
        url: '/message/sendMessage',
        data: data
      })
        .then((res) => {
          socket.emit('send-message', res.data);

        })

      input.value = ''
    }
  });

  // handle socketio
  socket.on('getMessage', (data) => {
    const boxMessage = document.querySelector('.chatBox__body')
    console.log(guestId)
    const item = document.createElement('div')
    item.classList.add('container__messages')

    item.innerHTML = `
                   <div class="messageWrapper ${guestId !== data.createdBy ? 'own' : ''} "> 
                        <div class="message-img">
                          <img
                            src="data:image/png;base64,"
                            alt="">
                        </div>
                        <div class="messageWrapper-content">
                          <span class="messageWrapper-text">
                            ${data.content}
                          </span>
                        </div>
                        <div class="messageWrapper-time">
                          <span class="timeAgo"> 10 days</span> ago
                  </div>

            `
    boxMessage.appendChild(item)
    window.scrollTo(0, document.body.scrollHeight);
  })


</script>


